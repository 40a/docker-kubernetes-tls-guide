apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: /etc/kubernetes/ca.pem
    server: https://kube-apiserver.c.kubestack.internal:443
  name: production
contexts:
- context:
    cluster: production
    user: kubelet
  name: proxy-to-production
current-context: proxy-to-production
users:
- name: proxy
  user:
    client-certificate: /etc/kubernetes/proxy-client.pem
    client-key: /etc/kubernetes/proxy-client-key.pem
